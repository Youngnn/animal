void title()
{
	char *cat[] = { "                  1.강 아 지\n","                  2.고 양 이\n","                  3.간 식 게 임\n","                  4.가 이 드\n","                  5.느 낀 점\n","                  6.게 임 종 료\n" };

	printf("                ┌-───★☆성실한 반려동물 키워보기!☆★────┐\n");
	printf("                │                                                │\n");
	printf("                │          Λ＿Λ      (자네 인상이 참 선하군..) │\n");
	printf("                │         ( 'ㅅ' )                               │\n");
	printf("                │                                     How        │\n");
	printf("                │                       Λ＿Λ        To         │\n");
	printf("                │                      ( 'ㅅ' )       rais       │\n");
	printf("                │      Λ＿Λ                         companion  │\n");
	printf("                │     ( 'ㅅ' )                        animals    │\n");
	printf("                │                                     !!         │\n");
	printf("                │            Λ＿Λ                              │\n");
	printf("                │           ( 'ㅅ' )                             │\n");
	printf("                │                          (날 키워라 닝겐...)   │\n");
	printf("                └────────────────────────┘\n\n\n\n");

	for (int i = 0; i < sizeof(cat) / sizeof(*cat); i++)
	{
		printf(*(cat + i));
	}

	printf("                                              \n");
	printf("                                  번 호 선 택 : ");
	printf("☆☆★번호를 눌러주세요!★☆☆: ");

	scanf("%d", &number);

	system("cls");//이전 화면을 지우기

	if (number == 1)
	{
		DOG();
	}
	else if (number == 2)
	{
		CAT();
	}
	else if (number == 3)
	{
		GAME();
	}
	else if (number == 4)
	{
		GUIDE();
	}
	else if (number == 5)
	{
		REVIEW();
	}
	else if (number == 6)
	{

	}
	else
	{
		printf("잘못된입력입니다.\n\n");
		while (number = getchar() != '\n' && number != EOF);   // x의 입력된 값을 getchar에 넣어 \n이 아니거나 EOF(파일의 끝 (입력의 끝))가 아닐때까지 반복
		title();
	}


}

void DOG()
{
	printf("댕댕이를 고르셨군요!");
	printf("┌────────────────────────────────────────────────┐\n");
	printf("│당신이 키우고 싶은 댕댕이는 무엇인가요?:\n1.대형견\n2.중형견\n3.소형견\n4.뒤 로                 │\n");
	printf("└────────────────────────────────────────────────┘\n");
	scanf("%d", &size);
	system("cls");
	if (size == 1)
	{
		BDOG();
	}
	else if (size == 2)
	{
		MDOG();
	}
	else if (size == 3)
	{
		SDOG();
	}
	else if (size == 4)
	{
		title();
	}
	else
	{
		printf("잘못된입력입니다.\n\n");
		while (size = getchar() != '\n' && size != EOF);   // x의 입력된 값을 getchar에 넣어 \n이 아니거나 EOF(파일의 끝 (입력의 끝))가 아닐때까지 반복
		DOG();
	}
}

void BDOG()
{
	printf("대형견을 선택하셨습니다! 관심있는 아이를 선택해주세요.\n");
	printf("☆대형견들은 대체적으로 성격이 차분하며 흥분도가 낮습니다!☆\n");
	puts("");
	printf("┌────────────────────┐\n");
	printf("│1. 골든 리트리버                        │\n");
	printf("│2. 올드 잉글리쉬 쉽독                   │\n");
	printf("│3. 달마시안                             │\n");
	printf("│4. 호바와트                             │\n");
	printf("│5. 사모예드                             │\n");
	printf("│6. 뒤 로                                │\n");
	printf("└────────────────────┘\n");
	printf("원하는 대형 댕댕이를 선택하세요.\n");
	scanf("%d", &bdog);
	system("cls");

	if (bdog == 1)
	{
		BDOG1();
	}
	else if (bdog == 2)
	{
		BDOG2();
	}
	else if (bdog == 3)
	{
		BDOG3();
	}
	else if (bdog == 4)
	{
		BDOG4();
	}
	else if (bdog == 5)
	{
		BDOG5();
	}
	else if (bdog == 6)
	{
		DOG();
	}
	else
	{
		system("cls");//이전 화면을 지우기
		printf("잘못된입력입니다.\n\n");

		BDOG();
	}
}

void BDOG1()
{
	printf("특징\n");
	puts("");
	printf("┌──────────────────────────────────────┐\n");
	printf("│평균 분양가격은 30~60만원대 사이 정도입니다.                                │\n");
	printf("│평균 몸무게는 28~33 kg 내외의 대형견입니다.                                 │\n");
	printf("│부드러운 성격을 원하는 분들이 분양받기에 적절합니다.                        │\n");
	printf("│높은 지능으로 안내견과 탐지견과 같은 역할을 수행합니다.                     │\n");
	printf("│평소 호기심이 많다. 지능과 사회성 발달을 위해 잦은 산책이 필요합니다.       │\n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	printf("주요 물품\n");
	puts("");
	printf("┌───────────────────────────────────────┐\n");
	printf("│주기적인 브러쉬질이 필요하니 되도록 입양과 동시에 장모용 브러쉬를 사용해라.   │\n");
	printf("│슈퍼 프리미엄과 홀리스틱 등급의 사료가 적절하다.                              │\n");
	printf("│강한 힘을 가지고 있으므로 일반적인 목줄보단 가슴줄을 이용해서 다루는게 좋다.  │\n");
	printf("│관리가 쉽지 않으므로 메가 사이즈의 케이지를 이용해야한다.                     │\n");
	printf("└───────────────────────────────────────┘\n");
	puts("");

	printf("주의 사항\n");
	puts("");
	printf("┌──────────────────────────────────────┐\n");
	printf("│교감이 뛰어나기로 소문난 품종으로 사회성의 발달을 위해 잦은 산책이 필요하다.│\n");
	printf("│혼자 있을 때 분리불안과 우울증에 걸리기 쉬워 6시간이상 떨어지지 않도록 한다.│\n");
	printf("│통계상 과반수 이상이 암으로 사망한다.                                       │   \n");
	printf("│건강에 예민하니 주기적인 건강검진은 필수다.                                 │\n");
	printf("│긴털 때문에 피부병에 걸리기 쉬워 케이지는 일주일마다 소독을 해줘야 한다.    │\n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	printf("분양 환경\n");
	puts("");
	printf("┌──────────────────────────────────────┐\n");
	printf("│활동적인 성격으로 넓은 범위의 환경에서 기르는 것이 중요하다.                │\n");
	printf("│사교성과 책임감이 강한 품종으로 교감을 할 만한 어린아이나                   │\n");
	printf("│다른 강아지들을 함께 키우는 것도 도움이 된다.                               │\n");
	printf("└──────────────────────────────────────┘\n");
	printf("\n");


	BDOGB();
}

void BDOG2()
{
	printf("특징\n");
	puts("");
	printf("┌──────────────────────────────────────┐\n");
	printf("│크기 55~60cm 체중 23~30 정도의 거구를 자랑하는 대형견이다.                  │\n");
	printf("│몸집이 매우 단단하며 몸 전체에 근육이 골고루 발달되어 있습니다.             │\n");
	printf("└──────────────────────────────────────┘\n");


	printf("주의 사항\n");
	puts("");
	printf("┌──────────────────────────────────────┐\n");
	printf("│앞발과 턱이 매우 발달하여 평소 훈련에 주의하는 편이 좋습니다.               │\n");
	printf("│코는 크고 검은색을 띠며 어렸을 때 꼬리를 잘라주는 습관이 있어               │ \n");
	printf("│촘촘히 난 잔털과 간간히 난 긴털이 조화를 이루어 고고한 모습이다.            │\n");
	printf("│얌전하고 조용한 성격이지만 고집이 매우 세 어렸을 때 조기 교육이 중요합니다. │\n");
	printf("└──────────────────────────────────────┘\n");
	puts("");
	printf("분양 환경\n");
	puts("");
	printf("┌──────────────────────────────────────┐\n");
	printf("│훈련을 잘 시키면 평생 주인만을 따르며 주위 환경에도 잘 적응 할 것입니다.    │\n");
	printf("│과거 목양견으로써 길러진 습성이 남아있어 넓은 마당에서 기르는걸 추천한다.   │\n");
	printf("└──────────────────────────────────────┘\n");
	puts("");
	

	BDOGB();
}

void BDOG3()
{
	printf("특징\n");
	puts("");
	printf("┌──────────────────────────────────────┐\n");
	printf("│달마시안은 크기 58~63정도의 대형견입니다.                                   │\n");
	printf("│체중의 균형이 잘 잡혀있고 탄탄한 몸매를 보여주는 품종입니다.                │\n");
	printf("│몸에 지방이 많지않고 대표적인 바둑무늬 얼룩점이 나있으며 모양은 제각각이다. │\n");
	printf("│이 얼룩점은 시간이 지나 강아지가 자라며 차츰 눈에띄게 짙어지기 시작합니다.  │\n");
	printf("│충성심이 높고 어른스러운 모습을 보이지만 호기심이 왕성하고 활동적이다.      │\n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	printf("분양 환경\n");
	puts("");
	printf("┌──────────────────────────────────────┐\n");
	printf("│가만히 있는 것을 싫어하고 어린아이들과 교감하며 노는 것을 좋아합니다.       │\n");
	printf("│장거리 마차를 호위하던 개여서 운동을 좋아하고 활기있게 운동하기를 좋아한다. │\n");
	printf("│사교성과 의젓함을 길러주기 위해 늘 곁에 두고 함께 운동을 해주는 것이 좋다.  │\n");
	printf("└──────────────────────────────────────┘\n");
	puts("");


	BDOGB();
}

void BDOG4()
{
	printf("특징\n");
	puts("");
	printf("┌──────────────────────────────────────┐\n");
	printf("│헌신적이며 조용한 성격이고 경계심이 많으며 침착하고 또한 활동적입니다.      │\n");
	printf("│크기는 68~73정도인 대표적 대형견이고 수명은 10~14년 정도다.                 │\n");
	printf("│대표 색상은 블랙과 버건디로 짙은 색을 자랑하는 고급스러운 품종입니다.       │\n");
	printf("│수십년 동안 추적, 수색견으로서 명성을 이어왔던 호바와트는 단순한            │\n");
	printf("│호기심으로 접근하기엔 초보견주에게 적합하지 않습니다.                       │\n");
	printf("│몸의 체중의 균형과 스트레스 해소를 위해 하루평균 2시간이상의                │\n");
	printf("│운동이 필요하고 주인과의 충분한 교감은 필수입니다.                          │\n");
	printf("│성격은 전투적이지 않고 쉽게 흥분하지 않으며 소유욕이 적고                   │\n");
	printf("│용맹하여 위엄있게 행동하고 자부심이 강해 늘 지능적입니다.                   │\n");
	printf("│주인에게 자연스럽게 순종하는 품종이며 2세를 낳을 땐까진 늘 어른스럽다.      │\n");
	printf("│수줍음을 잘 타고 과묵하지만 환경에 따라 유쾌한 가족견으로 성장할 수도 있다. │\n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	printf("\n");

	BDOGB();
}

void BDOG5()
{
	printf("특징\n");
	puts("");
	printf("┌──────────────────────────────────────┐\n");
	printf("│추위에 강하고 용맹하며 사역견으로서 길러져 사람에게 매우 친화적입니다.      │\n");
	printf("│원산지는 러시아로 늘 웃는 얼굴을 하고 있어 '미소천사'라는 별명으로 불린다.  │\n");
	printf("│크기는 45~50으로 평균적인 크기의 대형견이고 가정환경이 매우 중요시 됩니다.  │\n");
	printf("│더위에 약하므로 충분한 환경조성과 운동량의 분배와 먹이의 구성도 중요합니다. │\n");
	printf("│에너지 소비가 충분히 되지 않으면 아무리 온순해도 말썽을 부릴 수 있다.       │\n");
	printf("│사역견이라 사람에게는 온순하지만 다른견이나 어린아이와 서열정리를           │\n");
	printf("│하기 때문에 새끼때부터 사회화 교육은 필수입니다.                            │\n");
	printf("│더위를 먹지않게 통풍이 잘 될 수 있도록 해주어야 피부병을 예방할 수 있습니다.│\n");
	printf("│실내에서 키우신다면 대리석 소재의 바닥을 추천 드립니다.                     │\n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	printf("\n");
	BDOGB();
}

void BDOGB()
{
	printf("\n뒤로이동하고싶으면 0번 처음화면으로 이동하고 싶으실시 1번입니다.\n");
	printf("다른것을 입력할시 종료합니다\n");
	scanf("%d", &x);

	system("cls");
	if (x == 0)
	{
		BDOG();
	}
	else if (x == 1)
	{
		title();
	}
	else
	{
		exit(0);
	}
}

void MDOG()
{
	printf("중형견을 선택하셨습니다! 관심있는 아이를 선택해주세요.\n");
	printf("☆대형견들은 대체적으로 성격이 차분하며 흥분도가 낮습니다!☆\n");
	puts("");
	printf("┌─────────────────┐\n");
	printf("│1. 차우차우                       │\n");
	printf("│2. 올드 잉글리쉬 쉽독             │\n");
	printf("│3. 달마시안                       │\n");
	printf("│4. 호바와트                       │\n");
	printf("│5. 사모예드                       │\n");
	printf("│6. 뒤 로                          │\n");
	printf("└─────────────────┘\n");
	puts("");

	printf("원하는 대형 댕댕이를 선택하세요.\n");
	scanf("%d", &mdog);
	system("cls");

	if (mdog == 1)
	{
		MDOG1();
	}
	else if (mdog == 2)
	{
		MDOG2();
	}
	else if (mdog == 3)
	{
		MDOG3();
	}
	else if (mdog == 4)
	{
		MDOG4();
	}
	else if (mdog == 5)
	{
		MDOG5();
	}
	else if (mdog == 6)
	{
		DOG();
	}
	else
	{
		system("cls");//이전 화면을 지우기
		printf("잘못된입력입니다.\n\n");

		MDOG();
	}
}

void MDOG1()
{
	printf("특징\n");
	puts("");
	printf("┌──────────────────────────────────────┐\n");
	printf("│원산지는 중국으로 성격은 신경질 적이며 경계심이 많습니다.                   │\n");
	printf("│크기는 중형견으로, 체고 수컷 48~56cm·암컷 46~51이고 무게는 평균 20~32kg 정도입니다.\n");
	printf("│차우차우는 긴털로 눈병에 걸리기 쉬우며 워낙 고집이 센 성격이어서 움직이기 싫어해 고관절형성이상과 연구개과장증, 내분비계질환등 여러 질병에 걸리기 쉽습니다.\n");
	printf("│털이 매우 긴 편이어서 내한성이며 눈속에서 자도 멀쩡할 만큼 많은 지방과 털을 가지고 있습니다\n");
	printf("│여러 질병과 체격유지를 위해 하루평균 자전거 반주 정도의 산책을 약 30분씩 2회가량 필요로하니 항상 관심을 가져주세요\n");
	printf("│털이 엉키고 관리가 제대로 되지 않으면 피부병에 걸리기 쉬우니 장모타입의 털을 빗을 때 유리한 핀 브러시를 이용해 뭉친 털을 제거하거나 마사지에도 이용해주세요\n");
	printf("│매우 경계심이 많아 과잉방어를 하는 경향이 강하니 주의해주세요\n");
	printf("│복슬복슬한 털이 봉제인형 같은 차우차우는 일반적으로 귀염상으로도 잘 알려진 견종입니다.\n");
	printf("│3000년 전부터 존재했던 원산지 중국에서는 사냥이나 썰매를 끄는 등의 활약을 했는데 식용견으로 이용된 역사도 있습니다.\n");
	printf("│마치 새끼 곰처럼 귀엽고 사랑스러운 모습과는 달리 매우 신경질적인 성격이니 주의해 주세요.\n");
	printf("│공격적이지는 않지만 경계심이 강하기 때문에 시야 밖에서 갑자기 다가가거나 만지면 과잉반응하여 공격적으로 돌변하기도 한답니다.\n");
	printf("│주인이나 가족에게는 신뢰를 가지고 대하고, 기본적으로는 얌전한 성격입니다.\n");
	printf("│새끼 때부터 운동이 부족하거나 식생활에 문제가 있다면 비만이 되거나 고관절트러블 등이 발생할 수 있으니 건강관리에 신경을 써주어야 합니다.\n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	MDOGB();
}

void MDOG2()
{
	printf("특징\n");
	puts("");
	printf("┌──────────────────────────────────────┐\n");
	printf("│원산지는 스코틀랜드로 일부 견종들 보다 지능이 높고 천진난만하며 사람에게 매우 친숙해 주인을 잘 따릅니다.\n");
	printf("│크기는 수컷 53cm·암컷 53cm 미만에 이르며 몸무게는 14~20kg에 육박하는 중형견입니다.\n");
	printf("│활동적인 성격으로 운동을 좋아하지만 무리한 운동은 고질병인 관절질환을 야기 할 수있으므로 늘 주의를 해주는 것이 좋습니다.\n");
	printf("│추위를 특별히 타지 않는 품종이며 평소 운동을 매우 좋아하니 자전거 반주 정도의 산책을 1일 2회 약 한시간씩 해주는 것이 좋습니다.\n");
	printf("│털이 긴 품종이므로 장모 타입의 핀 브러시를 이용해 주시고 피부를 자주 마사지 해주는 편이 좋습니다.\n");
	printf("│프리스비 등 만능 스포츠견종을 통틀어 이 정도로 운동능력이 뛰어난 개는 없을 것입니다.\n");
	printf("│온몸이 용수철처럼 움직이는 보더 콜리는 프리스비 대회나 어질리티 경기 등에서 항상 상위에 랭크됩니다.\n");
	printf("│보더 콜리의 '보더'는 국경을 뜻하는데, 스코틀랜드와 잉글랜드와의 국경 부근에서 탄생해 목양견으로서 멋진 능력을 발휘하였다고 알려졌습니다.\n");
	printf("│지금도 목양견 중 가장 뛰어난 견종으로 인정받고 있으며 가축을 컨트롤하는 기술이 천하일품입니다.\n");
	printf("│지능이 높고 상황판단이 뛰어나며 천진난만하고, 사람에게 공격적인 면을 전혀 보이지 않습니다.\n");
	printf("│주인을 신뢰하고 아이들과도 잘 놀아주니 가정견으로서도 최고라고 할 수 있습니다.\n");
	printf("│운동신경이 좋다고 해서 무턱대고 운동을 시키다가는 관절질환과 피부질환에 걸릴 수 있으니 과격한 운동은 피하는 것이 좋아요.\n");
	printf("│산책은 매일 아침저녁으로 하루 2회, 자전거 등을 이용해 구보를 포함한 운동을 충분히 시켜줘야 합니다.\n");
	printf("│훈련시키기도 쉽고 습득도 빨라서 다양한 훈련의 재미를 주는 견종이니 함께 행동하면서 다양한 기술을 익히게 하면 주인과 개 모두에게 바람직합니다.\n");
	printf("└──────────────────────────────────────┘\n");
	puts("");


	MDOGB();
}

void MDOG3()
{
	printf("특징\n");
	puts("");
	printf("┌──────────────────────────────────────┐\n");
	printf("│원산지는 미국으로 성격은 사람을 잘 따르고 쾌활합니다.│\n");
	printf("│크기는 수컷 약 38.1cm·암컷 약 35.6cm 정도이고 무게는 11~13kg 나가는 중형견입니다.│\n");
	printf("│크기는 수컷 약 38.1cm·암컷 약 35.6cm 정도이고 무게는 약 11~13kg정도를 자랑하는 중형견입니다.│\n");
	printf("│듬직한 체구의 모습에 비해 슬개골탈구 지루증 갑상선기능저하 외이염 백내장 등 여러 가지 질병에 취약하므로 늘 신경을 써주는 것이 좋습니다.│\n");
	printf("│운동을 특별히 신경 써주지 않아도 균형적인 몸매를 유지시켜 줄 순 있지만 보통걸음으로 20분정도 걷는 산책을 1일 2회 평균으로 해주는 것이 바람직합니다.│\n");
	printf("│슬리커 브러시나 핀 브러시를 이용해 늘 털을 손질 해주는 것이 바람직합니다.\n");
	printf("│1620년경 영국에서 건너온 이민자들과 함께 미국 땅을 밟았던 \n");
	printf("│잉글리시 코커스패니얼 중에서 얼굴이 둥근 개끼리 재교배시켜 탄생한 것이 아메리칸 코커스패니얼입니다.\n");
	printf("│잉글리시 코커스패니얼과 가까운 친척관계인 만큼 이 두 견종의 생김새는 많이 비슷합니다.\n");
	printf("│아메리칸 코커는 코끝이 둥글고, 잉글리시 코커는 코끝이 길다는 것이 구분 포인트이죠.\n");
	printf("│해외의 도그쇼에서는 아메리칸 코커와 잉글리시 코커와의 차이가 명확하지만, 한국 내에서는 코끝의 차이가 분명하지 않습니다.\n");
	printf("│하지만 두 종류 모두 명랑 쾌활하면서 다정한 성격으로 사람과 함께 있는 것을 매우 좋아한답니다.\n");
	printf("│실크처럼 아름다운 털은 프로 트리머에게 손질을 맡기는 것이 편합니다.\n");
	printf("│늘어진 귀를 가졌고 피부가 약한 개체가 많으니 외이염이나 알레르기성 피부염 등에 주의해야 합니다.\n");
	printf("│훈련의 이해도 좋고 똑똑한 견종이지만, 어리광도 능숙하므로 주인이 끌려 다니지 않도록 조심해야 합니다.\n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	MDOGB();
}

void MDOG4()
{
	printf("특징\n");
	puts("");
	printf("┌──────────────────────────────────────┐\n");
	printf("원산지는 영국으로 호기심이 왕성하며 주인에게는 상냥하고 애정이 깊지만, 섬세하고 신경질적이며 조급한 성격입니다.\n");
	printf("크기는 수컷 약 41cm·암컷 41cm 이하 정도이고, 무게는 8.2~10.4kg 정도를 자랑하는 중형견입니다.\n");
	printf("피부질환이나 유루증, 정유고환등 여러 가지 질병에 약하고 능 민감하니 분양환경에 신경을 써주어야 합니다.\n");
	printf("내한성으로 추위를 특별히 느끼지 않는 품종입니다.\n");
	printf("운동량은 빠른걸음으로 30분정도 걷는 산책을 1일 2회가량 필요로 합니다.\n");
	printf("털이 긴 장모 타입의 견종이니 슬리커 브러시를 이용해 털의 엉킴을 없애고 빠진털을 주기적으로 제거해주어야 합니다.\n");
	printf("화려한 외모는 기품이 넘치지만 신경질적이고 거칠며 조급한 성격이다.\n");
	printf("감수성이 풍부하여 마음을 허락한 주인에게는 깊은 애정을 보이지만, 낯선 사람이 거슬리는 짓을 하면 맹렬하게 화를 내며 공격적인 모습을 보이기도 한다.\n");
	printf("조상은 탄광노동자들이 밀렵이나 투견으로 이용할 목적으로 키우던 개로 알려져 있는데 거친 성격은 이 견종의 뿌리에서 비롯된 것 같다.\n");
	printf("견종명은 영국의 노썸버랜드 주 베드린튼 시 주변의 사람들이 키운 데서 유래되었다.\n");
	printf("뛰어난 운동신경을 가져 움직임이 매우 빠르며 운동시킬 때는 자유운동을 포함한 산책이 좋다.\n");
	printf("복슬복슬한 털은 정기적으로 트리밍하는 것이 좋은데, 특히 눈 주변의 털이 눈에 들어가지 않도록 신경 쓴다.\n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	MDOGB();
}

void MDOG5()
{
	printf("특징\n");
	puts("");
	printf("┌──────────────────────────────────────┐\n");
	printf("원산지는 헝가리로 성격은 다정하며 가족을 생각하는 마음이 강해 동시에 충성도도 높지만 자존심이 매우 강합니다.\n");
	printf("중형견으로 크기는 수컷 39~45cm·암컷 36~42cm 정도로 자라며 무게는 수컷 13~15kg·암컷 10~13kg정도 나갑니다.\n");
	printf("걸리기 쉬운 질병은 관절질환, 안질환, 피부질환 정도에 취약합니다.\n");
	printf("내한성으로 추위에 매우 강하기로 소문난 품종입니다.\n");
	printf("빠른걸음으로 30분정도 걷는 산책을 1일 2회정도 해주면 좋습니다.\n");
	printf("털이 잘 엉키지 않아 손질을 거의 필요로 하지 않습니다.\n");
	printf("티베탄 스패니얼이 선조가 아닐까 추정되는 독특한 스타일의 풀리는 헝가리에서는 약 1000년 전부터 목축견으로 일했던 견종이다.\n");
	printf("자랑인 드레드헤어는 가축을 지킬 때, 추위로부터 몸을 지키거나 적과의 싸움에서 상대의 이가 살까지 파고들지 못하도록 돕는다.\n");
	printf("매우 영리하고 기억력이 좋아서 훈련은 힘들지 않다.\n");
	printf("주인 가족을 사랑하고 주의 깊은 면이 있으므로 가정견으로서도 훌륭한 번견 역할을 해낼 것이다.\n");
	printf("하지만 자존심이 강한 편이므로 무시하거나 필요 이상으로 야단을 치면 주눅이 들기 때문에 무조건 칭찬하여 재능을 향상시키는 것이 바람직하다.\n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	MDOGB();
}

void MDOGB()
{
	printf("\n뒤로이동하고싶으면 0번 처음화면으로 이동하고 싶으실시 1번입니다.\n");
	printf("다른것을 입력할시 종료합니다\n");
	scanf("%d", &x);

	system("cls");
	if (x == 0)
	{
		MDOG();
	}
	else if (x == 1)
	{
		title();
	}
	else
	{
		exit(0);
	}
}

void SDOG()
{
	printf("소형견을 선택하셨습니다! 관심있는 아이를 선택해주세요.\n");
	printf("☆소형견들은 대체적으로 성격이 쾌활하며 자존심이 강합니다!☆\n");
	puts("");
	printf("┌────────────────────────────┐\n");
	printf("1. 일본 스피츠\n");
	printf("2. 비숑\n");
	printf("3. 시바견\n");
	printf("4. 요크셔테리어\n");
	printf("5. 포메라니안\n");
	printf("6. 뒤 로\n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	printf("원하는 소형 댕댕이를 선택하세요.\n");
	scanf("%d", &sdog);
	system("cls");

	if (sdog == 1)
	{
		SDOG1();
	}
	else if (sdog == 2)
	{
		SDOG2();
	}
	else if (sdog == 3)
	{
		SDOG3();
	}
	else if (sdog == 4)
	{
		SDOG4();
	}
	else if (sdog == 5)
	{
		SDOG5();
	}
	else if (sdog == 6)
	{
		DOG();
	}
	else
	{
		system("cls");//이전 화면을 지우기
		printf("잘못된입력입니다.\n\n");

		SDOG();
	}
}

void SDOG1()
{
	printf("특징\n");
	puts("");
	printf("┌──────────────────────────────────────┐\n");
	printf("원산지는 일본으로 늘 쾌활하며 주인과 놀기 좋아하지만 낯가림이 심한 편입니다.\n");
	printf("수컷은 30~38cm, 암컷은 수컷보다 약간 작고.무게는 약 5~6kg으로 소형견입니다.\n");
	printf("약한 피부로 피부질환에 걸리기 쉬우며 그에 반해 내한성으로 추위에 매우 강합니다.\n");
	printf("내한성인 만큼 많은 지방량을 보유하고 있기 때문에 빠른걸음으로 30분이상 걷는 운동을 하루에 2회 이상 해주어야 합니다.\n");
	printf("장모 타입의 풍성한 털을 주기적으로 관리해주어야 하기 때문에 슬리커 브러시를 이용하여 털의 엉킴을 없애고 빠진 털을 제거해 주세요\n");
	printf("푹신하고 부드러운 순백의 털로 둘러쌓여 눈의 요정이라 불리는 재패니즈 스피츠의 인기는 한결같지만, 한때는 좋지 않은 이미지가 있었습니다.\n");
	printf("실외사육을 하는 번견으로 키우던 일본에서는 30년 전만 해도 어디서나 이 개를 흔히 볼 수 있었습니다.\n");
	printf("이런 인기를 앞세워 무분별하게 번식시킨 결과 신경질적인 면이 전면에 드러나면서 잘 짖고 잘 무는 개라는 오명이 붙게 되었습니다.\n");
	printf("하지만 해외에서 많은 사랑을 받으며 착실하게 사육된 결과 주인에게 복종하고 명랑한 본래의 성격으로 돌아올 수 있었습니다.\n");
	printf("1920년경 시베리아를 경유해 일본에 들어온 독일산 대형 흰 스피츠가 선조이며,\n");
	printf("그 후 일본에서 독자적으로 개량되었는데 스피츠라는 이름은 러시아어로 '불'을 의미하는 '스피츠'가 원어라고 하는데, \n");
	printf("불이 붙은 듯 시끄럽게 짖어대는 데서 유래됬다고 합니다.\n");
	printf("복슬복슬하고 새하얀 털은 보기보다 손질이 간편하지만 더러워지기 쉬우므로 정기적으로 목욕을 해주는 것이 메우 중요합니다.\n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	SDOGB();
}

void SDOG2()
{
	printf("특징\n");
	puts("");
	printf("┌──────────────────────────────────────┐\n");
	printf("원산지는 벨기에와 프랑스로 성격은 얼굴 그래도 명랑하고 다정하며 감수성이 풍부하고 똑똑합니다.\n");
	printf("크기는 최고 30cm로 밖에 자라지 않을 만큼 작은 몸집의 소형견이며 몸무게는 3~6kg정도 밖에 나가지 않습니다.\n");
	printf("태생부터 약하게 키워져 온 비숑은 관절이 매우 약한 편이라 관절질환에 걸리기 쉽고 피부질환또한 걸리기 쉽습니다.\n");
	printf("추위에 특별히 약하지 않은 평균적인 내한성 품종으며 일반 평균적인 견들과 마찬가지로 보통으로 10분 정도 걷는 가벼운 산택을 1일2회 가량 필요로 합니다.\n");
	printf("장모타입의 털을 가다듬기에 편리한 일자빗과 슬리커 브러시를 사용하는 편이 관리에 좋고\n");
	printf("특유의 털 윤기와 풍성함을 유지시키기 위해선 자주 털 손질을 해주어야 합니다.\n");
	printf("왕후나 귀족이 안던 개로 유명세를 가지고 있으며 가늘고 푹신푹신한 순백의 털로 감싸여 마치 봉제인형처럼 귀여운 비숑 프리제는,\n");
	printf("한때 유럽 귀부인들 사이에서 초상화에 함께 그려 넣는 것이 유행했을 정도로 사랑받았던 품종입니다.\n");
	printf("환자가 난방기구 대신 끌어안고 지냈을 만큼 안기는 것을 싫어하지 않는 견종이며 사람에게 매우 친화적이고 안기는 개이기는 하지만 약하지는 않습니다.\n");
	printf("포근한 털 밑에 숨겨진 근육질의 몸은 매우 단단하고 튼튼하며 명랑 활발하고 감수성도 풍부하다.\n");
	printf("운동량이 많이 필요하지 않고, 털빠짐이 적으며 체취도 심하지 않아서 실내견으로 최적으로 불리며,\n");
	printf("주인을 제일 소중하게 여기므로 마음을 치유해주는 좋은 상대가 되어줄 것입니다.\n");
	printf("대서양의 카나리아 제도에 서식하던 옛 토착견의 후손으로 추정되는 비숑 프리제는 프랑스어로 '곱슬털로 장식하다'는 뜻입니다.\n");
	printf("이름대로 언더코트는 조밀하면서도 부드럽고, 그것을 덮는 윗털은 부드럽고 곱슬거리며,\n");
	printf("긴 털로 덮인 꼬리도 풍성하여 날개털의 장식처럼 흔들리는 모습이 매우 기품이 넘치는 아름다운 견공입니다.\n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	SDOGB();
}

void SDOG3()
{
	printf("특징\n");
	puts("");
	printf("┌──────────────────────────────────────┐\n");
	printf("원산지는 일본(혼슈, 시코쿠의 산악지대)이며 성격은 주인 생각이 극진한 충견으로 경계심이 매우 강합니다.\n");
	printf("크기는 수컷 38.5~41.5cm·암컷 35.5~38.5cm로 무게는 9~14kg까지 나가는 소형견입니다.\n");
	printf("산간지방에서 자라 일반적인 품종들보다 내성이 강하며 피부질환에는 예민한 편이니 관리에 신경을 써주어야 합니다.\n");
	printf("산간지방에서 자란만큼 추위에 매우 강한 내한성 견공이며 평소 운동하기를 즐겨 \n");
	printf("견주 께서는 빠른 걸음으로 30분정도 걷는 산책을 1일 2회 정도 해주시는게 좋습니다\n");
	printf("단모 타입 개의 털에 윤기를 내거나 피부마사지에 효과가 좋은 솔 브러시를 이용해 관리해 주시고 우리나라를 비롯한 해외에서도 인기인 일본 대표 견종입니다.\n");
	printf("일본을 대표하는 개로, 특히 영국과 유럽 각국의 도그쇼에 상당수의 개체가 참가하고 있을 만큼 일본은 물론 해외에서도 매우 인기가 높다.\n");
	printf("해외에서의 호칭도 역시 '시바견'으로 불립니다, 고대 일본에서 키웠던 역사 깊은 견종으로,\n");
	printf("아이치 현의 상흑암 유적에서 시바견과 비슷한 견종의 뼈가 발견되어 조선시대에 일본으로 건너온 사람들과 함께 오지 않았을까 추정되는 견종입니다.\n");
	printf("오랜 역사를 가진 이 귀한 견종은 1937년 일본의 천연기념물로 지정 되었습니다.\n");
	printf("고온다습하면서도 겨울에는 추위가 심한 일본의 사계절 기후에 알맞게 매우 튼튼한 견종이지만,\n");
	printf("최근에는 무분별한 번식으로 인해 경계심이 강하고 신경질적이고 헛울음이 많고 공격적인 개체도 증가하고 있는 상황입니다.\n");
	printf("민첩하고 활동적인 시바견 에게는 일일 운동량을 거르지 않아야 합니다.\n");
	printf("주인과 함께 산책하고 행동하면서 최고의 만족감과 충실감을 느끼기 때문에!\n");
	printf("매일 다양한 산책코스로 변화를 준다면 함께 놀라거나 감동하는 등 여러 가지 체험을 공유할 수 있을 것입니다.\n");
	printf("└──────────────────────────────────────┘\n");
	puts("");


	SDOGB();
}

void SDOG4()
{
	printf("특징\n");
	puts("");
	printf("┌──────────────────────────────────────┐\n");
	printf("원산지는 영국에서 물먹고 넘어온 강아지로 성격은 주인과 함께 있으면 세상 높은줄 모르고 드세며 괘활해집니다\n");
	printf("크기는 23cm 전후를 달리며 무게는 3.1kg 이내로 작은 체구를 자랑하는 소형견입니다.\n");
	printf("매우 작은 품종인 만큼 몸이 약해 여러 가지 질병에 걸리기 쉽지만 대표적으로도 슬개골 탈구와 저혈당증에 주의해야 합니다.\n");
	printf("추위에 매우 약한 품종으로 겨울에는 특히 더 난방에 신경쓰며 길러줘야 하고, 보통 10분정도 가볍게 걷는 운동을 1일 2회정도 해주시면 좋습니다.\n");
	printf("장모 타입의 풍성한 털을 손질하기 위해 슬리커 브러시를 이용해주면 좋고 특유의 윤기 유지를 위해 털 건강에도 신경을 써주시는 편이 좋습니다.\n");
	printf("마치 실크처럼 매끄러운 광택의 코트를 가진 요크셔테리어는 '요키'라는 애칭으로 친숙하며,\n");
	printf("항상 인기순위의 상위를 유지해왔으며 기네스북에는 가장 작은 개로 공인된 개체의 기록도 있습니다.\n");
	printf("조상은 쥐를 잡기 위해 만들어낸 견종이며, 실제로도 쥐사냥의 달인으로 불립니다.\n");
	printf("스코틀랜드에서 잉글랜드로 이주해온 이 개를 19세기 중엽 요크셔 지방의 공업지대에서 일하던 사람들이 쥐사냥 용도로 집에서 키웠는데 당시는 지금보다 훨씬 더 몸이 컸다고 합니다\n");
	printf("그 후 유럽 상류계급에서 '움직이는 보석'이라는 별명이 붙으면서 인기가 높아졌고 어느새 기품 있는 견종으로 통하게 되었습니다\n");
	printf("품격은 높아졌지만 성격이 크게 변하지 않은 요키는 쥐사냥의 피가 끓는지 경계심과 승부욕이 강하고 시끄럽게 잘 짖습니다.\n");
	printf("주인과 함께 있을 때는 쾌활하고 종종걸음으로 잘 돌아다니지만,\n");
	printf("응석받이에 외로움도 잘 타 장시간 혼자 두거나 펫호텔에 맡기면 갑자기 건강이 나빠지기도 하니 주의를 기해야하고 또 낯선 사람을 심하게 경계하는 모습도 보입니다.\n");
	printf("잘못 키워 어리광을 부리게 하거나 제멋대로인 성격을 방치하면 주인에게도 짖어대는 신경질적인 개가 될 수 있으니\n");
	printf("강아지 때부터 엄격한 훈육이 중요하며 주인은 주도권을 잃어서는 안 됩니다.\n");
	printf("요키의 아름다운 코트 손질은 간단하지 않습니다.\n");
	printf("내버려두면 털이 지저분하게 쑥쑥 자라므로 아름다움을 유지하려면 자주 트리밍해줘야 하며,\n");
	printf("특히 얼굴 주변의 털이 눈에 들어가거나 식사 때 음식물이 묻으면 피부병의 원인이 됩니다.\n");
	printf("자라난 털은 잘 정리해서 끈으로 묶어주는 것도 좋다 이 래핑 방법은 주인도 집에서 쉽게 할 수 있습니다.\n");
	printf("태어난 직후 입과 다리 외의 피모는 새까만데, 생후 2세까지는 피모의 색깔을 확실하게 알 수 없습니다.\n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	SDOGB();
}

void SDOG5()
{
	printf("특징\n");
	puts("");
	printf("┌──────────────────────────────────────┐\n");
	printf("원산지는 독일로 성격은 쾌활하고 호기심이 왕성하지만 신경질적인 면도 있습니다.\n");
	printf("크기는 18~22cm 밖에 되지 않으며 무게는 1.8~2.3kg 밖에 나가지 않는 정말 작은 소형견입니다.\n");
	printf("슬개골탈구, 유루증, 기관허탈, 골절 등 여러 가지 질병에 취약하지만 추위에는 강한 견종이니 평소 운동을 시켜 균형있는 근육량을 유지해주는 것이 중요합니다.\n");
	printf("따라서 보통 10분정도 걷는 가벼운 운동을 1일1회가량 해주는 것이 좋습니다.\n");
	printf("이쁘기로 소문난 푸메라니안의 장모 털을 관리해주기 위해선 핀 브러시나 슬리커 브러시를 이용하는 것이 편리 합니다.\n");
	printf("조그만 체구에 귀여운 종종걸음으로 돌아다니며, 탐스러운 털을 나부끼는 모습이 오랫동안 높은 인기를 구가해온 비결일 것입니다.\n");
	printf("호기심이 왕성해서 흥미진진하게 참견하는 모습이 또 밉지 않습니다.\n");
	printf("반면 신경질적인 면이 있어서 새끼 때 커뮤니케이션이 부족하거나 제멋대로 키우면, 요구를 충족시켜줄 때나 낯선 사람이 올 때 하염없이 짖기도 하고,\n");
	printf("공포를 느끼면 공격적이 될 수 있으니 주의해야 합니다.\n");
	printf("자기 뜻대로 되지 않으면 다루기 힘들고 다소 자기중심적인 견종입니다.\n");
	printf("북방 스피츠 계열인 사모예드가 뿌리가 된 목양·작업견이 직계 조상으로 추정됩니다.\n");
	printf("당시에는 상당히 대형이었지만, 후일 독일의 포메라니아 지방에서 소형화되었고 개량지의 지명에서 견종명이 유래되었다.\n");
	printf("추위를 견딜 수 있는 북방 스피츠의 더블코트를 물려받은 포메라니안의 털은 개량이 가미되어 부드럽고 상당히 가는 것이 특징이지만,\n");
	printf("개가 아파할 정도로 무리하게 빗질을 하면 섬세한 털이 찢어지기도 한다.\n");
	printf("그렇게 되면 탐스러운 털이 무용지물이 될 뿐 아니라 빗질을 싫어하게 될 수도 있다.\n");
	printf("이 아름다운 털을 유지하려면 매일 빗질을 거르지 않아야 하는데 일자빗이나 슬리커 브러시로 부드럽게 빗질하여 흐트러진 털이 없도록 정돈해줘야 합니다.\n");
	printf("일일 운동량은 많이 필요하지 않지만, 기분전환이나 스트레스 발산을 위해서 잠깐이라도 외출하는 것이 좋습니다.\n");
	printf("하지만 너무 심한 운동은 작은 몸과 가느다란 다리에 오히려 부담을 주게 되는데,\n");
	printf("주인에게 안겨 외출하던 포메라니안은 뛰어내리다가 골절 등의 사고를 당할 수도 있습니다.\n");
	printf("실제로 포메라니안에게 발생하는 대개의 트러블이 골절인 만큼 신경 써야 할 부분입니다.\n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	SDOGB();
}

void SDOGB()
{
	printf("\n뒤로이동하고싶으면 0번 처음화면으로 이동하고 싶으실시 1번입니다.\n");
	printf("다른것을 입력할시 종료합니다\n");
	scanf("%d", &x);

	system("cls");
	if (x == 0)
	{
		SDOG();
	}
	else if (x == 1)
	{
		title();
	}
	else
	{
		exit(0);
	}
}

void CAT()
{


	printf("┌──────────────────────────────────────┐\n");
	printf("주인님을 고르셨습니다.\n\n");
	printf("주인님 털 종류를 고르세요: \n 1. 단 모 \n 2. 장 모\n 3. 뒤 로 \n");
	printf("└──────────────────────────────────────┘\n");
	puts("");


	scanf("%d", &tcat);
	system("cls");//이전 화면을 지우기

	if (tcat == 1)
	{
		SCAT();
	}
	else if (tcat == 2)
	{
		LCAT();
	}
	else if (tcat == 3)
	{
		title();
	}
	else
	{
		printf("잘못된입력입니다.\n\n");
		while (tcat = getchar() != '\n' && tcat != EOF);   // x의 입력된 값을 getchar에 넣어 \n이 아니거나 EOF(파일의 끝 (입력의 끝))가 아닐때까지 반복
		CAT();
	}
}

void SCAT()
{
	printf("☆단모를 선택하셨습니다.☆ \n");
	printf("원하시는 주인님의 번호를 입력해주십시오.\n");
	printf("\n");

	printf("┌──────────────────────────────────────┐\n");
	printf("1.코리아 숏헤어 \n");
	printf("2.러시안 블루 \n");
	printf("3.스코티시 폴드\n");
	printf("4.아비시니안\n");
	printf("5.뱅 갈\n");
	printf("6.샤미즈 샴\n");
	printf("7.아메리칸 숏헤어\n");
	printf("8.뒤 로\n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	printf("원하는 고양이를 선택하세요.\n");
	scanf("%d", &scat);
	system("cls");

	if (scat == 1)
	{
		KCAT1();
	}
	else if (scat == 2)
	{
		KCAT2();
	}
	else if (scat == 3)
	{
		KCAT3();
	}
	else if (scat == 4)
	{
		KCAT4();
	}
	else if (scat == 5)
	{
		KCAT5();
	}
	else if (scat == 6)
	{
		KCAT6();
	}
	else if (scat == 7)
	{
		KCAT7();
	}
	else if (scat == 8)
	{
		CAT();
	}
	else
	{
		system("cls");//이전 화면을 지우기
		printf("잘못된입력입니다.\n\n");
		SCAT();
	}

}

void KCAT1()
{
	printf("┌──────────────────────────────────────┐\n");
	printf("코리아 숏헤어:\n");
	printf("원산지: 한국\n");
	printf("발생: 자연발생\n");
	printf("성격: 활발, 대담, 감정변화가 큰 편 \n");
	printf("크기: 중형 \n");
	printf("무게: 약 4~8kg \n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	printf("\n");


	printf(" 특징\n");
	printf("┌──────────────────────────────────────┐\n");
	printf("고양이 협회에 등록된 순종은 아니지만 우리나라를 포함하여  \n");
	printf("전 세계적으로 분포하고 있으며 아프리카가 원산지인 것으로 추정된다.   \n");
	printf("외모와 성격을 단정지을 수 없을 만큼 종류가 다양하다. \n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	printf("\n");
	
	printf(" 외모 & 털\n");
	printf("┌──────────────────────────────────────┐\n");
	printf("주변에서 흔히 보는 길고양이는 모두 코숏이다. 노란 태비, 삼색이, 줄무늬, 얼룩이 등 끝도 없이 다양하다. \n");
	printf("자연적으로 발생한 품종이기 때문에 단모종이며 추위를 견디기 위해 속털이 나 있다.  \n");
	printf("└──────────────────────────────────────┘\n");
	puts("");




	printf(" 성격 \n");
	printf("┌──────────────────────────────────────┐\n");
	printf("보통 7주 이전에 사람 손을 타면 사람을 잘 따르는 집고양이가 된다. \n");
	printf("사람에게 크게 상처를 입지 않은 한 대부분은 순하고 친절하며 영민하다.\n");
	printf("└──────────────────────────────────────┘\n");
	puts("");


	KCATB();
}

void KCAT2()
{
	printf("┌──────────────────────────────────────┐\n");
	printf("러시안블루 \n");
	printf("원산지: 러시아 \n");
	printf("발생: 자연발생, 인위적발생 \n");
	printf("성격: 소심, 어리광쟁이 \n");
	printf("크기: 중형 \n");
	printf("무게: 4~8kg \n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	


	printf("특징  \n");
	printf("┌──────────────────────────────────────┐\n");
	printf(" 제2차 세계대전 이후에 거의 사라진 고양이의 혈통 보전을 위해 영국 고양이와 다시 교배했다. \n");
	printf(" 러블이라 불리는 이름처럼 사랑스럽고 사람의 감정을 읽는 능력이 매우 뛰어나다. \n");
	printf("└──────────────────────────────────────┘\n");
	puts("");




	printf("외모 & 털  \n");
	printf("┌──────────────────────────────────────┐\n");
	printf(" 청회색의 오묘한 털과 에메랄드색의 눈을 가졌으며 몸매가 날렵하며 근육질이다.다른 묘종에 비해 크기가 작은 편이다. \n");
	printf(" 러시안 블루는 눈 색깔이 두 번 변하는 것으로 유명한데 생후 2개월쯤 되었을 때 청회색 눈이 노란색으로 바뀌고, 6개월쯤 되었을 때 다시 한 번 초록색으로 바뀐다. \n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	printf("\n");


	printf("성격  \n");
	printf("┌──────────────────────────────────────┐\n");
	printf(" 순하고 애교가 많지만 낯선 사람은 경계하는 편이다. \n");
	printf(" 기분을 파악하는 데 뛰어난 능력을 가지고 있어서 주인이 우울해하면\n");
	printf(" 다가와 온몸을 비비면서 위로를 하는 놀라운 능력을 지녔다. \n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	KCATB();
}

void KCAT3()
{
	printf("┌──────────────────────────────────────┐\n");
	printf("스코티시 폴드  \n");
	printf("원산지: 영국 \n");
	printf("발생:돌연변이 \n");
	printf("성격: 온화, 다루기 쉬움 \n");
	printf("크기: 중형 \n");
	printf("무게: 약 4~7kg \n");
	printf("└──────────────────────────────────────┘\n");
	puts("");




	printf("특징  \n");
	printf("┌──────────────────────────────────────┐\n");
	printf("얼굴과 눈이 동그랗고 귀가 앞으로 처져 있어 늘 놀란 표정, 억울한 표정을 하고 있다. \n");
	printf("귀여운 외모 때문에 세계적으로 마니아층이 가장 두텁다. \n");
	printf("돌연변이로 발생한 품종이기 때문에 기형 관절 등의 유전성 질환이 있다. \n");
	printf(" 귀가 접힌 암수가 교배할 경우 아기 고양이에게 뼈 이상이 와서 짧은 다리로 \n");
	printf(" 잘 걷지 못하는 일이 발생할 수도 있기 때문에 귀가 접힌 고양이끼리는 교배시키지 않는다. \n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	


	printf("외모 & 털  \n");
	printf("┌──────────────────────────────────────┐\n");
	printf(" 얼굴과 눈이 동그랗고 귀가 접혔으나 간혹 귀가 접히지 않은 스코티시 폴드도 있다.생후 3~4주가 되면 귀가 말리다가 3개월 때의 귀 형태가 평생 지속된다. \n");
	printf("  단모종과 장모종이 있으나 우리나라에 있는 스코티시는 대부분 단모종이다.털색은 갈색과 회색 등 다양하다. \n");
	printf("귀가 접혀있지 않은 고양이는 스코티시 스트레이트라고 부른다. \n");
	printf("└──────────────────────────────────────┘\n");
	puts("");




	printf("성격  \n");
	printf("┌──────────────────────────────────────┐\n");
	printf("조용하고 얌전하며 사람을 잘 따른다. \n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	KCATB();
}

void KCAT4()
{
	printf("┌──────────────────────────────────────┐\n");
	printf("아비시니안 \n");
	printf("원산지: 영국 \n");
	printf("발생: 자연발생 \n");
	printf("성격: 어리광쟁이, 장난꾸러기 \n");
	printf("크기: 중형 \n");
	printf("무게: 약 4~8kg \n");
	printf("└──────────────────────────────────────┘\n");
	puts("");


	printf("특징  \n");
	printf("┌──────────────────────────────────────┐\n");
	printf("피라미드에서 출토된 고양이 모양의 조각상들은 아비시니안을 매우 닮았으며\n");
	printf("고대 이집트의 벽화에서도 비슷한 고양이 모습을 확인할 수가 있다. \n");
	printf(" 눈 주변에 아이라인과 비슷한 선이 있는데 이것이 이집트 벽화에 보이는 짙은 \n");
	printf(" 눈 화장의 원조가 되었다는 설까지 있을 만큼 고대 이집트에서는 이 고양이가 신성시되었다. \n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	printf("\n");


	printf("외모 & 털  \n");
	printf("┌──────────────────────────────────────┐\n");
	printf("털이 굵고 뻣뻣하며, 몸매가 늘씬하고 호리호리하다. \n");
	printf("장모종과 단모종이 있는데 장모종인 경우에는 ‘소말리’라고 한다. \n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	

	printf("성격  \n");
	printf("┌──────────────────────────────────────┐\n");
	printf("외모는 고급스럽지만 성격은 정반대이다. \n");
	printf("수다스럽고 분주해 잠시도 가만히 있지 않고 돌아다니며 장난을 친다. \n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	KCATB();
}

void KCAT5()
{
	printf("┌──────────────────────────────────────┐\n");
	printf("뱅갈 \n");
	printf("원산지: 영국 \n");
	printf("발생: 인위발생 \n");
	printf("성격: 민감, 자립심이 강하다 \n");
	printf("크기: 대형 \n");
	printf("무게: 약 5~10kg \n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	printf("특징  \n");
	printf("┌──────────────────────────────────────┐\n");
	printf("일본에서 인기 있는 품종으로 아프리카 밀림의 치타를 그대로 축소한 듯한 외모로 시선을 끈다. \n");
	printf(" 야생성, 근육질, 와일드한 외모로 인해 남자 애묘인에게 더 인기가 많다.우리나라에서는 대중적이지는 않으나 확실한 마니아층을 가진 품종이다. \n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	printf("\n");


	printf("외모 & 털  \n");
	printf("┌──────────────────────────────────────┐\n");
	printf("호피 문양의 털에 근육질의 몸매를 가졌으며 일반 고양이보다 크기가 다소 크다. \n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	printf("\n");

	printf("성격  \n");
	printf("┌──────────────────────────────────────┐\n");
	printf("잘 길들여지는 성격 때문에 강아지처럼 장난감을 물어 오게 하는 놀이도 가능하며 주인이 말을 시키면 대답을 하듯 소리를 낸다. \n");
	printf(" 물을 좋아하고 훈련 여부에 따라 목줄을 맨 채 산책을 할 수도 있다. \n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	KCATB();
}

void KCAT6()
{
	printf("┌──────────────────────────────────────┐\n");
	printf("샤미즈 샴 \n");
	printf("원산지: 태국 \n");
	printf("발생: 자연발생 \n");
	printf("성격: 어리광쟁이, 제멋대로 \n");
	printf("크기: 소형 \n");
	printf("무게: 약 3~6kg \n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	printf("특징  \n");
	printf("┌──────────────────────────────────────┐\n");
	printf("태국의 옛 왕국인 샴에서 왕들이 왕궁의 수호를 위해 키우던 고양이로 고급스럽고 멋지게 생겼다. \n");
	printf("단모종 중에서는 세계적으로 가장 인기 있는 품종이다.자연적으로 발생한 종이다. \n");
	printf("└──────────────────────────────────────┘\n");
	puts("");



	printf("외모 & 털  \n");
	printf("┌──────────────────────────────────────┐\n");
	printf("단모종으로 털이 빳빳하고 겉은 진한 갈색, 안쪽은 흰색이다. \n");
	printf(" 주둥이, 발, 꼬리 등 몸의 끝부분 색이 더 진한 것이 특징이다. \n");
	printf("진한 갈색과 크림색의 샤미즈가 있는데 어릴 때는 밝은 색이더라도 나이가 들면 색이 진해진다. \n");
	printf("└──────────────────────────────────────┘\n");
	puts("");



	printf("성격  \n");
	printf("┌──────────────────────────────────────┐\n");
	printf("영리하여 훈련이 가능하고 사람에게 매우 친절하지만 정신이 없고 시끄러운 편이다.\n");
	printf("국내에서는 ‘개냥이’라고 불릴 정도로 사람을 좋아하고 따른다. \n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	KCATB();
}

void KCAT7()
{
	printf("┌──────────────────────────────────────┐\n");
	printf(" 아메리칸 숏헤어\n");
	printf("원산지: 미국 \n");
	printf("발생: 자연발생 \n");
	printf("성격:  대담, 활발\n");
	printf("크기: 중형 \n");
	printf("무게: 약 6~8kg \n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	printf("\n");


	printf(" 특징\n");
	printf("┌──────────────────────────────────────┐\n");
	printf(" 17세기에 쥐를 잡는 고양이로 유럽에서 미국으로 넘어왔다. \n");
	printf(" 쥐잡이 전문 고양이로서 굵은 뼈와 튼튼한 근육질을 자랑한다. \n");
	printf("└──────────────────────────────────────┘\n");
	puts("");



	printf(" 외모 & 털 \n");
	printf("┌──────────────────────────────────────┐\n");
	printf("순종 고양이이긴 하지만 일반 집고양이와 다름없다. \n");
	printf(" 털색과 문양이 굉장히 다양하다.\n");
	printf(" 차이가 있다면 일반 집고양이는 삼각형의 뾰족한 얼굴인 데 반해  \n");
	printf("아메리칸 숏헤어는 얼굴과 체형이 동그란 편이라 매우 귀여운 느낌을 준다. \n");
	printf("└──────────────────────────────────────┘\n");
	printf("\n");

	printf("성격\n");
	printf("┌──────────────────────────────────────┐\n");
	printf("활발하고 사람을 잘 따른다.  \n");
	printf("친절하지만 고양이로서의 자존심도 강해서 사람과 일정한 거리를 유지한다. \n");
	printf("└──────────────────────────────────────┘\n");
	printf("\n");

	KCATB();
}

void KCATB()
{
	printf("\n뒤로이동하고싶으면 0번 처음화면으로 이동하고 싶으실시 1번입니다.\n");
	printf("다른것을 입력할시 종료합니다\n");
	scanf("%d", &x);

	system("cls");
	if (x == 0)
	{
		SCAT();
	}
	else if (x == 1)
	{
		title();
	}
	else
	{
		exit(0);
	}
}

void LCAT()
{
	printf("☆장모를 선택하셨습니다.☆ \n");
	printf("원하시는 주인님의 번호를 입력해주십시오.\n");
	printf("\n");
	printf("┌──────────────────────────────────────┐\n");
	printf("1.노르웨이 숲 \n");
	printf("2. 터키시 앙고라\n");
	printf("3.히말라얀\n");
	printf("4.페르시안\n");
	printf("5.랙 돌\n");
	printf("6.시베리안\n");
	printf("7.버 만\n");
	printf("8.뒤 로\n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	printf("원하는 고양이를 선택하세요.\n");
	scanf("%d", &lcat);
	system("cls");

	if (lcat == 1)
	{
		LCAT1();
	}
	else if (lcat == 2)
	{
		LCAT2();
	}
	else if (lcat == 3)
	{
		LCAT3();
	}
	else if (lcat == 4)
	{
		LCAT4();
	}
	else if (lcat == 5)
	{
		LCAT5();
	}
	else if (lcat == 6)
	{
		LCAT6();
	}
	else if (lcat == 7)
	{
		LCAT7();
	}
	else if (lcat == 8)
	{
		CAT();
	}
	else
	{
		system("cls");//이전 화면을 지우기
		printf("잘못된입력입니다.\n\n");
		LCAT();
	}

}

void LCAT1()
{
	printf("┌──────────────────────────────────────┐\n");
	printf("노르웨이 숲  \n");
	printf("원산지: 노르웨이 \n");
	printf("발생: 자연발생 \n");
	printf("성격: 대담, 침착한 면이 있음 \n");
	printf("크기: 대형 \n");
	printf(" 무게 : 약 5~10kg \n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	printf("특징  \n");
	printf("┌──────────────────────────────────────┐\n");
	printf("노르웨이에서 자연적으로 발생한 품종으로 북유럽의 추운 날씨를 견디기 위해 굵고 긴털이 몸을 덮고 있다. \n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	printf("외모 & 털  \n");
	printf("┌──────────────────────────────────────┐\n");
	printf("몸집이 크고 근육질로 이루어져 튼튼하다.귀에는 장식모가 나 있고 중장모이다. \n");
	printf(" 고급스럽고 우아한 줄무늬를 가지고 있으며, 부드럽고 빽빽한 속털과 \n");
	printf(" 	기름기 있는 겉털이 보온과 방수를 겸한다.털은 잘 뭉치는 편은 아니다. \n");
	printf("└──────────────────────────────────────┘\n");
	puts("");
	
	printf("성격  \n");
	printf("┌──────────────────────────────────────┐\n");
	printf("야생성이 남아 있어서 나무 타기, 공놀이 등 다소 과격한 놀이를 즐기며 매우 활발하다. \n");
	printf("사람을 좋아하며 친절하지만 애교 있는 성격은 아니다. \n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	LCATB();
}

