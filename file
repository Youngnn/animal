	LCATB();

}

void LCAT3()
{

	printf("┌──────────────────────────────────────┐\n");
	printf("히말라얀  \n");
	printf("원산지: 영국, 미국 \n");
	printf("발생: 인위발생 \n");
	printf("성격: 태평, 우아 \n");
	printf("크기: 대형 \n");
	printf("무게: 약 4~8kg \n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	printf("특징  \n");
	printf("┌──────────────────────────────────────┐\n");
	printf("히말라얀은 페르시안과 샤미즈를 인공 교배하여 만든 품종이다. \n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	printf("외모 & 털  \n");
	printf("┌──────────────────────────────────────┐\n");
	printf("얼굴 모양은 페르시안을 닮았고, 털색은 샤미즈를 닮았으며 장모종이다. \n");
	printf(" 포인트를 가진 페르시안 고양이라고 볼 수 있는데 페르시안의 한 갈래로 보는 고양이 협회도 있다. \n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	printf("성격  \n");
	printf("┌──────────────────────────────────────┐\n");
	printf("조용하고 순하기로 유명한 페르시안과 인간 친화적이기로 유명한 샤미즈를 섞어 놓았기에 성격이 그 어떤 품종보다 훌륭하다. \n");
	printf("심지어 큰 수술을 마치고 입원해 있는 동안 엄청난 통증에 시달리면서도 끊임없이 가르릉거리고\n");
	printf(" 꾹꾹이를 하며 모든 의료진에게 얼굴을 비비는 모습을 보여 주는 등 낙천적인 성격을 지녔다. \n");
	printf("샤미즈의 유전자를 받아 페르시안보다는 조금 더 활동적이다. \n");
	printf("└──────────────────────────────────────┘\n");
	puts("");

	LCATB();

}

