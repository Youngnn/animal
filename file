	printf("└──────────────────────────────────────┘\n");
	puts("");


	LCATB();
}

void LCATB()
{
	printf("\n뒤로이동하고싶으면 0번 처음화면으로 이동하고 싶으실시 1번입니다.\n");
	printf("다른것을 입력할시 종료합니다\n");
	scanf("%d", &x);

	system("cls");
	if (x == 0)
	{
		LCAT();
	}
	else if (x == 1)
	{
		title();
	}
	else
	{

		while (number = getchar() != '\n' && number != EOF);   // x의 입력된 값을 getchar에 넣어 \n이 아니거나 EOF(파일의 끝 (입력의 끝))가 아닐때까지 반복
		exit(0);
	}
}

void GUIDE()
{
	printf("☆가이드를 보고싶으시군요.☆ \n");
	printf("언제나 이 가이드를 생각하며 아이를 입양해주세요!.\n");
	printf("\n");
	printf("┌──────────────────────────────────────┐\n");
	printf("1.강아지 유의사항 \n");
	printf("2.고양이 유의사항\n");
	printf("3.뒤 로\n");
	printf("└──────────────────────────────────────┘\n");
	printf("알고싶은 정보를 선택하세요.\n");
	puts("");

	scanf("%d", &guide);
	system("cls");

	if (guide == 1)
	{
		DGUIDE();
	}
	else if (guide == 2)
	{
		CGUIDE();
	}

	else
	{
		system("cls");//이전 화면을 지우기
		printf("잘못된입력입니다.\n\n");
		title();
	}
}

void DGUIDE()
{
	printf("염두해야할 것\n");
	puts("");
	printf("┌──────────────────────────────────────┐\n");
	printf("강아지를 키우기 전 가장 염두해야 할 것은 자신이 견주로써 시작하기 전 얼마나 강아지에 대해 알고 있는지를 인지하는 것이 중요하다.\n");
	printf("당신이 상상하는 그저 귀여운 모습만을 가지고 있는 아이들은 절대로 아니며 각각의 개성을 가지고 있는 만큼 자존심이 강할 수도, 심각하게 소극적일 수도 있다.\n");
	printf("그런 아이들을 당신이 주인으로써, 친구로써 함께 살아가기 위해선 당신이 먼저 그들에 대해 정확히 알고 충분한 준비를 해야 할 필요가 있음을 강조하는 것이다.\n");
	printf("└──────────────────────────────────────┘\n");
	puts("");


	puts("");
	printf("언어\n");
	printf("┌──────────────────────────────────────┐\n");
	printf("우선 언어에 대해 이야기를 시작하자면 만인이 알고 있듯, 강아지가 꼬리를 흔든다는건 상대방을 매우 반긴다는 의미이며,\n");
	printf("동시에 당신의 반려견이 지금 매우 기분이 좋다는 것을 의미한다.\n");
	printf("그러나 다른 한편으로 당신이 그 아이를 너무 방치해서 격하게 반기는 것은 아닐까 사랑에 대한 간절함을 표현하는 것은 아닐까 늘 고민해 보아야한다.\n");
	printf("또한 강아지는 단순한 행동을 반복함으로써 자신의 감정을 나타내곤 한다.\n");
	printf("이것은 흔히 말하는 몸의 언어가 될 수 있고 혹은 강아지가 당신에게 관심을 요하며 지르는 장난이 될 수 있다.\n");
	printf("흔히 사람들이 강아지를 분양받는 곳은 펫샷 혹은 주변에서의 권유다.\n");
	printf("그러나 이런 식의 분양은 대부분 생후 2개월정도의 어린 아이들을 분양받게 되고 그저 어리기만 한 강아지들은 사람들의 장난감으로 전락해버릴 수도 있다.\n");
	printf("걸음마를 시작한 아이에게 당장 뛰라고 할 수 없듯 그저 힘겹게 감정을 표현하는 아이에게 많은 걸 바라진 말자.\n");
	printf("└──────────────────────────────────────┘\n");
	puts("");


	printf("물품\n");
	printf("┌──────────────────────────────────────┐\n");
	printf("장모는 슬리커 브러시 단모는 핀 브러시를 이용해 털을 관리해 주는 것이 좋다.\n");
	printf("또한 대형견은 프리미엄 사이즈의 케이지를 이용해 늘 편안하게 쉴 수 있는 공간을 마련해 주어야한다.\n");
	printf("강아지는 늘 당신의 눈치를 보며 행동하고 있음을 잊지말고 사랑으로 아껴주자.\n");
	printf("└──────────────────────────────────────┘\n");
	puts("");




}

void CGUIDE()
{
	printf("순종인가, 잡종인가 \n");
	puts("");
	printf("┌──────────────────────────────────────┐\n");
	printf("순종 고양이를 키우고 싶다면 전문 브리더를 통해서 분양을 받아야 하며 \n");
	printf("흔한 품종이 아니라면 시간이 좀 걸리더라도 자신이 원하는 품종이 들어오거나 태어날 때까지 기다리는 게 좋다. \n");
	printf("단, 대부분의 전문 브리더들은 분양 시 중성화를 시키는 것을 조건으로 걸어 혈통을 보존하므로 이는 염두에 두고 있어야 한다. \n");
	printf("순종 고양이의 경우 몇 가지 종 특이적인 질병이 있을 수 있으니 동물 병원에 방문하여 이를 점검해 보아야 한다. \n");
	printf("혈통보다는 인간과의 교감을 중시한다면 동물 보호소 등을 통해 유기묘를 입양하는 것도 좋은 방법이다. \n");
	printf("└──────────────────────────────────────┘\n");
	puts("");


	printf("고양이를 기르기 전에 염두에 두어야 할 것들 \n");
	printf("자신의 건강 \n");
	puts("");
	printf("┌──────────────────────────────────────┐\n");
	printf("천식, 비염, 알레르기가 있다면 아무리 고양이를 좋아한다고 해도 고양이를 키우지 않는 것이 좋다. \n");
	printf("이미 알레르기가 있는 사람은 물론이고 알레르기가 없던 사람도 고양이와 생활을 하면서 서서히 증상이 생기는 경우도 있다. \n");
	printf("└──────────────────────────────────────┘\n");
	puts("");


	printf("고양이 털과 위생 \n");
	puts("");
	printf("┌──────────────────────────────────────┐\n");
	printf("고양이의 털은 당신이 무엇을 생각하든 그 이상이다.그만큼 고양이는 털이 길고 많고 또 많이 빠지는 동물 중의 하나이다. \n");
	printf("특히 장모종을 기르는 경우라면 검정색 옷을 입는 것은 상상할 수도 없는 일이다. \n");
	printf("혹시 입는다면 수없이 털이 붙는 것을 감수해야 한다.고양이 털만 문제가 되는 것은 아니다. \n");
	printf("지나치게 깔끔하면서 고양이를 키우기는 힘든 일이다. \n");
	printf("신발장에서 뒹굴고 놀다가 침대에 걸터앉는 고양이를 만나게 될 수도 있다. \n");
	printf("어디든 자유자재로 오를 수 있는 데다 행동도 민첩한 고양이에게는 전기 철조망을 치지 않는 한 행동에 제약을 주기가 쉽지 않다. \n");
	printf("뿐만 아니다.고양이 모래로 인해 방이 항상 버석거리는 경우도 감수해야 한다. \n");
	printf("└──────────────────────────────────────┘\n");
	puts("");


	printf("경제적 비용 \n");

	puts("");
	printf("┌──────────────────────────────────────┐\n");
	printf("처음 고양이를 분양 받고자 할 때는 본인이 선택하기에 따라 비용의 차이는 크다.\n");
	printf("보편적으로 가정 분양을 받는 경우에는 30~40만 원, 펫숍에서 분양을 받는 경우에는 60~90만 원 정도 든다. \n");
	printf("물론 모래와 사료, 부수적인 고양이 용품까지 추가한다면 비용은 더 증가한다. \n");
	printf("또한 종합 백신, 광견병 백신, 심장 사상충과 외부 기생충 접종까지 포함한 초기 접종 비용은 약 16만 원 정도이다. \n");
	printf("중성화 수술의 경우 수고양이는 혈액 검사 비용을 제외하고 7~20만 원 정도, 암고양이의 경우 15~35만 원 정도로 생각하면 된다. \n");
	printf("또 고양이에 따라 병치레를 한두 번은 하기 마련인데 병의 경중에 따라 비용이 천차만별이 될 수 있다. \n");
	printf("아직까지는 반려 동물의 대부분을 강아지가 차지하고 있지만, 혼자 사는 젊은 남녀나, 맞벌이 부부에게는 오히려 고양이가 적합하다. \n");
	printf("고양이는 기본적으로 독립심이 강하기 때문에 낮에 종일 혼자 집에 남겨짐으로써 오는 심리적인 장애가 강아지보다 덜하다. \n");
	printf("└──────────────────────────────────────┘\n");
	puts("");


	printf("고양이는 감정을 다양하게 표현한다\n");
	puts("");
	printf("고양이의 경우에는 하루 종일 비어 있는 집의 창가에서 일광욕을 즐기거나 그루밍을 하며 혼자만의 시간을 즐기다가\n");
	printf("주인이 오면 슬금슬금 마중을 나와 한 번 스윽 쳐다보고는 하던 일을 계속한다.\n");
	printf("그렇다고 해서 고양이가 주인과의 교감이 전혀 없는 것은 아니다.\n");
	printf("오히려 고양이는 반려 동물 중에서 가장 자신의 감정을 정확하게 전달하는 동물이다.\n");
	printf("고양이가 기분이 나쁠 때는 ‘하악~’ 하는 소리를 내며 온몸의 털과 발톱을 세운다.\n");
	printf("또는 으르렁대는 소리도 내는데 가히 공포감을 불러일으킨다.\n");
	printf("그러나 고양이가 기분이 좋을 때는 지그시 두 눈을 감고 ‘가르릉’ 소리를 낸다.\n");
	printf("몸 안에서 울리는 이 소리는 주인과 교감을 하는 데 굉장히 중요한 수단이 된다.\n");
	printf("때로는 꾹꾹이 라고 해서 주인의 배나 팔 등 푹신한 부분을 꾹꾹 누르는 행동을 하기도 하고, \n");
	printf("우다다 라고 하여 야행성인 고양이가 밤중에 한 번씩 방 안을 마구 뛰어다니는 행동을 하기도 하는데 모두 컨디션이 무척 좋음을 뜻한다. \n");
	printf("그루밍이란 몸단장을 하는 행위를 일컫는데, 고양이는 하루 중에 많은 시간을 까슬까슬한 혀로 온몸의 털을 고르는 데 보낸다. \n");
	printf("이것이 고양이 몸에서 냄새가 잘 나지 않는 이유가 되기도 하고 ‘헤어볼’이라는 털 뭉치를 토하게 되는 원인이 되기도 한다.\n");
	printf("독립적인 성격과 풍부한 감정 표현 외에 반려 동물로서 또 다른 장점은 훌륭한 배변 습관에 있다.\n");
	printf("고양이는 본능적으로 배변을 가린다.딱히 가르치지 않아도 모래에만 대소변을 보며 모래를 덮어 냄새를 제거한다.\n");
	printf("고양이는 하루 종일 혼자 몸단장을 하기 때문에 몸에서 냄새가 나는 법이 없다.\n");
	printf("└──────────────────────────────────────┘\n");
	puts("");



	printf("필요한 물품\n");
	puts("");
	printf("┌──────────────────────────────────────┐\n");
	printf("사료: 어릴 때 많은 열량이 필요하기 때문에 키튼사료가 좋다.전연령 사료도 있다.\n");
	printf("그릇: 그릇은 높이 조절이 가능한 그릇이 굉장히 좋다.플라스틱보다는 스테인리스\n");
	printf("간식: 친해지기 위해서 가장 필요한 물품이다.하지만 생후 3개월 이후부터 주어야한다.\n");
	printf("화장실: 고양이는 매우 청결한 동물이기 때문에 화장실이 필요하다.\n");
	printf("삽: 모래속 똥과 오줌을 따로 분리해서 버려야할 때 꼭 필요하다.\n");
	printf("모래: 화장실안에 모래가 필요하다.\n");
	printf("빗: 털빠짐이 많아 이틀에 한번정도는 빗질을 해주어야한다.\n");
	printf("샴푸, 치약: 청결을 위해서\n");
	printf("장난감: 사람의 발, 손을 장난감으로 생각하고 깨무는 경우가 많아 꼭 필요하다.\n");
	printf("낚시대, 공등 고양이는 동그란 물체와 빠르게 움직이는것을 좋아한다.\n");
	printf("캣타워: 높은곳들을 좋아하기 때문에 주인님을 위한다면 추천한다.\n");
	printf("스크래쳐: 발톱도 갈고 스트레스를 풀기 위해서 꼭 필요하다.\n");
	printf("귀세척제: 벌레(기생충)가 생길수도 있다.\n");
	printf("발톱깎이: 날카롭지 않게 깎아야한다.긁히면 피를 보게된다.\n");
	printf("이동장: 집냥이들은 대부분 집 밖은 불안하기 때문에 싫어한다.\n");
	printf("집:푹신푹신하고 천장이 있는 종류의 집을 추천한다. 체온유지를 위해\n");
	printf("└──────────────────────────────────────┘\n");
	puts("");


	puts("");
}

void GUIDEB()
{
	printf("\n뒤로이동하고싶으면 0번 처음화면으로 이동하고 싶으실시 1번입니다.\n");
	printf("다른것을 입력할시 종료합니다\n");
	scanf("%d", &x);

	system("cls");
	if (x == 0)
	{
		GUIDE();
	}
	else if (x == 1)
	{
		title();
	}
	else
	{
		exit(0);
	}
}

void GAME()
{
	printf("간식을 줄 동물을 고르세요.\n");
	printf("┌──────────────────────────────────────┐\n");
	printf("1. 강아지\n2. 고양이\n3. 뒤 로\n");
	printf("└──────────────────────────────────────┘\n");
	puts("");


	scanf("%d", &ch);
	if (ch == 1)
	{
		DGAME();
	}
	else if (ch == 2)
	{
		CGAME();
	}
	else
	{
		exit(0);
	}
}

void DGAME()
{

	char *DSnack[] = { "    소고기 파우더       ",
		"   마도로스펫 북어트릿  ",
		"     팬시피스트 연어    ",
		"    화이크 밀크스틱     ",
		"        연어트릿        ",
		"    네이월 오리목뼈     ",
		"      헤어볼 참치       " };

	system("cls");		//헤더파일 Windows.h를 이용하여 화면을 지우는데 사용
	printf("                          ☆★☆★☆★☆★☆★☆★☆★\n");
	printf("                          ★어떤 간식이 나오냐?! (3)☆\n");
	printf("                          ☆★☆★☆★☆★☆★☆★☆★\n");
	Sleep(1000);		//헤더파일 Windows.h를 이용하여 Sleep을 사용해서 1초 지연시키는 프로그램
	system("cls");		//헤더파일 Windows.h를 이용하여 화면을 지우는데 사용
	printf("                          ☆★☆★☆★☆★☆★☆★☆★\n");
	printf("                          ★어떤 간식이 나오냐?! (2)☆\n");
	printf("                          ☆★☆★☆★☆★☆★☆★☆★\n");
	Sleep(1000);		//헤더파일 Windows.h를 이용하여 Sleep을 사용해서 1초 지연시키는 프로그램
	system("cls");		//헤더파일 Windows.h를 이용하여 화면을 지우는데 사용
	printf("                          ☆★☆★☆★☆★☆★☆★☆★\n");
	printf("                          ★어떤 간식이 나오냐?! (1)☆\n");
	printf("                          ☆★☆★☆★☆★☆★☆★☆★\n");
	Sleep(1000);		//헤더파일 Windows.h를 이용하여 Sleep을 사용해서 1초 지연시키는 프로그램

	system("cls");//헤더파일 Windows.h를 이용하여 화면을 지우는데 사용

	srand((unsigned int)time(NULL));   //srand로 시드(seed)값을 time을 사용해 초단위로 바꿔 랜덤으로 지정
									   //(time을 사용 안하면 시드(seed)값이 고정되어 같은 값만 나온다.)
	for (int i = 0; i < 1; i++) {      //반복문
		a = rand() % 7;      //a는 0 ~ 7 사이의 숫자를 랜덤으로 지정
		printf("                          ☆★☆★☆★☆★☆★☆★\n");
		printf("                          ★   %d번 간식 당첨   ☆\n", a + 1);      //숫자 a 확인
		printf("                          ☆★☆★☆★☆★☆★☆★\n");
		puts("");
		puts("");
	}//랜덤함수

	printf("┌───────────────────────────────┐\n");
	printf("│                   ");
	printf(*(DSnack + a));
	printf("                   │ \n");
	printf("└───────────────────────────────┘\n");

	GAMEB();
}

void CGAME()
{

	char *CSnack[] = { "          캣토리 고양이캔          ",
		"          챠오츄르 닭고기          ",
		"        굿프랜드 치킨츄 딩고껌     ",
		"         쉐바 쉬바 파우치 캔       ",
		"  아보덤 그레인프리 고양이주식캔   ",
		"          챠오츄르 참치맛          ",
		"            아이펫밀크             " };
	system("cls");		//헤더파일 Windows.h를 이용하여 화면을 지우는데 사용
	printf("                          ☆★☆★☆★☆★☆★☆★☆★\n");
	printf("                          ★어떤 간식이 나오냐?! (3)☆\n");
	printf("                          ☆★☆★☆★☆★☆★☆★☆★\n");
	Sleep(1000);		//헤더파일 Windows.h를 이용하여 Sleep을 사용해서 1초 지연시키는 프로그램
	system("cls");		//헤더파일 Windows.h를 이용하여 화면을 지우는데 사용
	printf("                          ☆★☆★☆★☆★☆★☆★☆★\n");
	printf("                          ★어떤 간식이 나오냐?! (2)☆\n");
	printf("                          ☆★☆★☆★☆★☆★☆★☆★\n");
	Sleep(1000);		//헤더파일 Windows.h를 이용하여 Sleep을 사용해서 1초 지연시키는 프로그램
	system("cls");		//헤더파일 Windows.h를 이용하여 화면을 지우는데 사용
	printf("                          ☆★☆★☆★☆★☆★☆★☆★\n");
	printf("                          ★어떤 간식이 나오냐?! (1)☆\n");
	printf("                          ☆★☆★☆★☆★☆★☆★☆★\n");
	Sleep(1000);		//헤더파일 Windows.h를 이용하여 Sleep을 사용해서 1초 지연시키는 프로그램

	system("cls");//헤더파일 Windows.h를 이용하여 화면을 지우는데 사용

	srand((unsigned int)time(NULL));   //srand로 시드(seed)값을 time을 사용해 초단위로 바꿔 랜덤으로 지정
									   //(time을 사용 안하면 시드(seed)값이 고정되어 같은 값만 나온다.)
	for (int i = 0; i < 1; i++) {      //반복문
		a = rand() % 8;      //a는 0 ~ 7 사이의 숫자를 랜덤으로 지정
		printf("                          ☆★☆★☆★☆★☆★☆★\n");
		printf("                          ★   %d번 간식 당첨   ☆\n", a);      //숫자 a 확인
		printf("                          ☆★☆★☆★☆★☆★☆★\n");
		puts("");
		puts("");
	}//랜덤함수

	printf("┌───────────────────────────────┐\n");
	printf("│              ");
	printf(*(CSnack + a));
	printf("             │ \n");
	printf("└───────────────────────────────┘\n");

	GAMEB();
}

void GAMEB()
{
	printf("\n뒤로이동하고싶으면 0번 처음화면으로 이동하고 싶으실시 1번입니다.\n");
	printf("다른것을 입력할시 종료합니다\n");
	scanf("%d", &x);

	system("cls");
	if (x == 0)
	{
		GAME();
	}
	else if (x == 1)
	{
		title();
	}
	else
	{

		while (x = getchar() != '\n' && x != EOF);   // x의 입력된 값을 getchar에 넣어 \n이 아니거나 EOF(파일의 끝 (입력의 끝))가 아닐때까지 반복
		exit(0);
	}


}

void REVIEW()
{

	printf(" 이름     학번     느낀점  \n");
	printf("  \n");
	struct account my = { "이제현", 1760044, "앞으로 조금 더 많은 경험을 통해 앞으로 더 좋은 프로그램을 만들고싶다." };

	struct account you = { "조영민", 1760050, "저번 학기 때 보다 더 많은 것을 배우고 직접 실습해보아서 더 좋은 경험이였다." };

	printf("%s %d %s\n", my.name, my.id, my.review);
	printf("  \n");
	printf("  \n");
	printf("%s %d %s\n", you.name, you.id, you.review);

	system("pause");         //계속하시려면 아무 키나 누르십시오..문장 출력
	system("cls");            //헤더파일 Windows.h를 이용하여 화면을 지우는데 사용
	title();
}

void EXIT()
{
	exit(0);
}
